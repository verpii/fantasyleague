<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="fantasyleague.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:fantasyleague"
    xmlns:view="clr-namespace:fantasyleague.V"
    Title="fantasyleague">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate view:HomePage}"
       
        Route="HomePage" />

    <ShellContent Title="My Team"
                      Icon="team.png" 
                      Route="team"
                      ContentTemplate="{DataTemplate view:MyTeamPage}" />

   

    <ShellContent Title="Market"
                      Icon="market.png"
                      Route="market" 
                      ContentTemplate="{DataTemplate view:Market}" />

  


</Shell>
